{
  "name": "physicianqa-filler",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "PhysicianQA Filler - A Vue 3 PWA for filling PDF templates",
  "scripts": {
    "dev": "vite",
    "dev:clean": "npm run clean:dev && vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean:dev": "rm -rf ./assets ./registerSW.js ./sw.js ./workbox-*.js ./debug.html ./manifest.webmanifest 2>/dev/null || true && cp index.development.html index.html"
  },
  "dependencies": {
    "@mdi/font": "^7.3.0",
    "pdf-lib": "^1.17.1",
    "vue": "^3.4.0",
    "vuetify": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "terser": "^5.43.1",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vite-plugin-vuetify": "^2.0.0"
  }
}
